    private void calculateActionPerformed(java.awt.event.ActionEvent evt) {                                          

        int eachslot=1;
      int synnoofframes=simduration;
        int syneachframe=3*8;
        int syntotbitinframe=24*synnoofframes;
        double outbw=150.0;double bwutilised;
        res.setText("SYNCHRONOUS TDM = "+syntotbitinframe+"\nSTATISTICAL TDM = "+statnbits);
double statbw,synbw;
statbw=((double)statnbits/outbw)*100;
synbw=((double)syntotbitinframe/outbw)*100;
  BufferedWriter out = null;
        try{
            FileWriter fWriter = new FileWriter("report.txt"); //Creates new file if already not exists
            out = new BufferedWriter(fWriter);
            out.write("*************RESULTS OF SYNCHRONOUS TIME DIVISION MULTIPLEXING**************");
            out.newLine();out.newLine(); // Writes new line
            out.write("Random data stream generated for 1st input line -- ");
            for(int g=0;g<in1;g++)
            {if(inputdata1[g]=='\0')
             out.append("empty"+" ");
             else
                out.append(inputdata1[g]+" ");}
            out.newLine();out.newLine();
            out.write("Random data stream generated for 2nd input line -- ");
            for(int g=0;g<in2;g++)
                {if(inputdata2[g]=='\0')
             out.append("empty"+" ");
             else
                out.append(inputdata2[g]+" ");}
            out.newLine();out.newLine();
            out.write("Random data stream generated for 3rd input line --");
            for(int g=0;g<in3;g++)
                {if(inputdata3[g]=='\0')
             out.append("empty"+" ");
             else
                 out.append(inputdata3[g]+" ");}
            out.newLine();out.newLine();
            out.write("Total no of frames generated");out.newLine();out.newLine();
            out.write("Total no of output frames = ");
            out.append(simduration+"");out.newLine();out.newLine();
            out.write("No of bits in each frame = ");out.append("24");out.newLine();out.newLine();
            out.write("Total no of output bits =");out.append(syntotbitinframe+" ");out.newLine();out.newLine();
             out.write("Bandwidth utilized = 3 bits per second");out.newLine();out.newLine();out.newLine();
            ////////////////////////////////////////////////////////////////////////////////////
            out.write("*************RESULTS OF STATISTICAL TIME DIVISION MULTIPLEXING**************");
            out.newLine();out.newLine();out.newLine(); // Writes new line
            out.write("Random data stream generated for 1st input line -- ");
            for(int g=0;g<in1;g++)
            {if(inputdata1[g]=='\0')
             out.append("empty"+" ");
             else
                out.append(inputdata1[g]+" ");}
            out.newLine();out.newLine();
            out.write("Random data stream generated for 2nd input line -- ");
            for(int g=0;g<in2;g++)
                {if(inputdata2[g]=='\0')
             out.append("empty"+" ");
             else
                out.append(inputdata2[g]+" ");}
            out.newLine();out.newLine();
            out.write("Random data stream generated for 3rd input line --");
            for(int g=0;g<in3;g++)
                {if(inputdata3[g]=='\0')
             out.append("empty"+" ");
             else
                 out.append(inputdata3[g]+" ");}
            out.newLine();out.newLine();out.newLine();
            out.write("Total no of frames generated");out.newLine();out.newLine();
            out.write("Total no of output frames = ");
            out.append(simduration+"");out.newLine();out.newLine();
            out.write("Total no of bits in the output frames = ");out.append(statnbits+"");out.newLine();out.newLine();
            out.write("**************Analysis of Bandwidth Utilization***************");out.newLine();out.newLine();
            out.newLine(); out.newLine();
            out.write(" Bandwidth utilization for SYNCHRONOUS TDM :");
            out.write(synbw+"%");
            out.newLine(); out.newLine();
            out.write(" Bandwidth utilization for STATISTICAL TDM :");
            out.write(statbw+"%");
             out.newLine(); out.newLine();


            if(statnbits<syntotbitinframe)
            {
                out.write("The Bandwidth in case of STATISTICAL TDM is used efiiciently for the above input data");
                out.newLine(); out.newLine();
                out.write("STATISTICAL TDM outperforms the Best");
            }
            if(statnbits>syntotbitinframe)
            {
                out.write("The Bandwidth in case of SYNCHRONOUS TDM is used efiiciently for the above input data");
                out.newLine(); out.newLine();
                out.write("SYNCHRONOUS TDM outperforms the Best");
            }
 if(statnbits==syntotbitinframe)
            {

                out.write("Both TDM outperforms the Best");
            }

        }catch(IOException es){

        }finally{

            if( out != null){
                try{
                    out.close();
                }catch(IOException es){

                }
            }
        }
JOptionPane.showMessageDialog(null,"Click on VIEW RESULTS to open the Result file");

 

        // TODO add your handling code here:
    }                                         

